{
  "manifest_version": 3,
  "name": "Add to Lailrecipes",
  "description": "Add the current recipe to the Lailrecipes app",
  "version": "1.1",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [ "activeTab" ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Add to my Recipes"
  }
}
